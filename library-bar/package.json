{
  "name": "library-bar",
  "version": "0.1.0",
  "author": "Jens Bodal",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf lib/* dist/*",
    "release": "npm run build && timestamp=$(date \"+%s\") && touch lib/$timestamp && npm pack",
    "watch": "nodemon -w src -e ts --exec 'npm run release'"
  },
  "devDependencies": {
    "@types/lodash-es": "4.17.3",
    "lodash-es": "4.17.15",
    "typescript": "3.8.3"
  }
}
